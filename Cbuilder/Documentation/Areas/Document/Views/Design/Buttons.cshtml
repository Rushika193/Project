

@{
    ViewData["Title"] = "Buttons";
}

<div class="cd_content">
    <h1 class="content_title ">Buttons</h1>
    <p class="content_lead ">
        Custom button styles for actions in forms, dialogs, and more with support for multiple styles and designs to be used wherever necessary.
    </p>
</div>

<div class="cd_example Mb-10x">
    <div class="preview">
        <div class="sfButtonwrapper ">

            <button class="btn primary-outline round">Primary</button>
            <button class="btn secondary-outline round">Secondary</button>
            <button class="btn success-outline round">Success</button>
            <button class="btn info-outline round">Info</button>
            <button class="btn warning-outline round">Warning</button>
            <button class="btn danger-outline round">Danger</button>
            <button class="btn light-outline round">Light</button>
            <button class="btn dark-outline round">Dark</button>

        </div>
    </div>
    <figure>
        <div data-lang="lang-html" class="codeContent">
            <button class="btn primary-outline round">Primary</button>
            <button class="btn secondary-outline round">Secondary</button>
            <button class="btn success-outline round">Success</button>
            <button class="btn info-outline round">Info</button>
            <button class="btn warning-outline round">Warning</button>
            <button class="btn danger-outline round">Danger</button>
            <button class="btn light-outline round">Light</button>
            <button class="btn dark-outline round">Dark</button>
        </div>
    </figure>
</div>

