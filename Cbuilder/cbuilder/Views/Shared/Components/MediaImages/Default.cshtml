@*@model Cbuilder.MediaManagement.Models.MediaFileInfo;*@
@model Cbuilder.Core.MediaManagement.MediaLibraryInfo


@if (Model.MediaCategories != null && Model.MediaCategories.Length > 0)
{
    for (int i = 0; i < Model.MediaCategories.Length; i++)
    {
        @await Component.InvokeAsync("MediaDOM", new { filePath = Model.MediaCategories[i], mediaSettingKeys = @Model.MediaSettingKeys })
    }
}
else
{
    <ul id="ulCategory">
        <li class="nomediaData"></li>
    </ul>
}

